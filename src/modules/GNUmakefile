TOPDIR = ../..

SUBDIRS = database audit authentication authorization filter ddlcompiler normalize prnt transaction cmdbind echoProcessor
ifeq ($(WITH_LUA),1)
SUBDIRS += lua
endif

-include $(TOPDIR)/makefiles/gmake/sub.mk

local_all:

local_clean:

local_distclean:

local_test:

local_install:
	$(INSTALL) -d -m 0755 $(DESTDIR)$(libdir)/wolframe/modules
	$(INSTALL) -d -m 0775 $(DYNAMIC_MODULE) $(DESTDIR)$(libdir)/wolframe/modules

local_uninstall:
	@-rmdir $(DESTDIR)$(libdir)/wolframe/modules
