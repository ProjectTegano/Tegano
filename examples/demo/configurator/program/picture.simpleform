DOCTYPE "picture Picture"
{
	picture
	{
		id @int
		caption string
		info string
		thumbnail string
		image string
		width int
		height int
		size int
		tags string
	}
	
	list
	{
		picture []
		{
			id @int
			caption string
			info string
			thumbnail string
			image string
			width int
			height int
			size int
			tags string
		}
	}
}
