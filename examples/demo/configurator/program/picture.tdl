TRANSACTION selectPictureList
BEGIN
	INTO /picture DO NONEMPTY SELECT ID AS "ID",thumbnail,caption,info,width,height,length(image) as size FROM Picture;
END

TRANSACTION selectPicture -- (/picture/id)
BEGIN
	INTO picture DO NONEMPTY UNIQUE SELECT ID AS "ID",image,caption,info,width,height FROM Picture WHERE ID = $(id);
END
