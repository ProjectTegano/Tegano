STRUCT PersonRef
{
	prename string
	surname string
}

STRUCT AddressRef
{
	street string
	town string
}

STRUCT CompanyRef
{
	name string
}

STRUCT Person
{
	company CompanyRef[]
	id int
	parent ?string
	child PersonRef[]
	prename string
	surname string
	location AddressRef[]
	tag ?int
}

STRUCT Company
{
	id int
	parent string
	name string
	location AddressRef[]
	child CompanyRef[]
	tag int[]
}

FORM Data
	:root data
{
	person Person[]
}

FORM AllDataRequest
	:root data
{
}



