FORM places
	:root PLACES
{
	GARDEN []
	{
		NAME string
		ADDRESS
		{
			COUNTRY int = 49
			STREET string
			CITY string
			PHONE {
				NUMBER string
				MOBILE string
			}
		}
		PLANT []
		{
			COMMON string[]
